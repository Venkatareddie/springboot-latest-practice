package com.streams;

public class MaxMinAvgSalary {
	public static void main(String[] args) {
		Employee j1 = new Employee("Venkat", 50000.00);

	}

class Employee {
	int empName;
	double empSal;

	public Employee(int empName, double empSal) {
		this.empName = empName;
		this.empSal = empSal;
	}

	@Override
	public String toString() {
		return "Employee [empName=" + empName + ", empSal=" + empSal + "]";
	}
}

}
